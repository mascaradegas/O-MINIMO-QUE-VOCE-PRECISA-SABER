{
  "name": "o-minimo-que-voce-precisa-saber",
  "version": "1.0.0",
  "description": "Backend API for English Course - Simple SQLite",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "dev": "node server.js",
    "dev:backend": "nodemon server.js",
    "dev:frontend": "cd english-page && npm run dev",
    "dev:all": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "test": "node --test tests/*.test.js",
    "test:watch": "node --test --watch tests/*.test.js",
    "test:all": "npm test && cd english-page && npm test",
    "test:panel": "node scripts/test-runner.js",
    "test:panel:verbose": "node scripts/test-runner.js --verbose",
    "test:frontend": "node scripts/test-runner.js --frontend",
    "test:backend": "node scripts/test-runner.js --backend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SevenZeroSev3n/O-MINIMO-QUE-VOCE-PRECISA-SABER.git"
  },
  "keywords": [
    "api",
    "express",
    "sqlite"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^12.5.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "winston": "^3.19.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "nodemon": "^3.1.11",
    "supertest": "^7.0.0"
  }
}
